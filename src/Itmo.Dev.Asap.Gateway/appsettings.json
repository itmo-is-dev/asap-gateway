{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.Extensions.Http": "Information",
        "System.Net.Http.HttpClient": "Information",
        "Grpc.AspNetCore.Server": "Information",
        "Grpc.Net.Client": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:T} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs/asap-gateway/AppLogs_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}",
          "retainedFileCountLimit": 30
        }
      }
    ]
  },
  "Sentry": {
    "Enabled": true,
    "Environment": "",
    "Dsn": "",
    "Debug": false,
    "TracesSampleRate": 1.0,
    "MinimumEventLevel": "Warning"
  },
  "Platform": {
    "Environment": "",
    "YandexCloud": {
      "ServiceUri": "",
      "LockBox": {
        "SecretId": ""
      }
    }
  },
  "AllowedHosts": "*",
  "Infrastructure": {
    "Grpc": {
      "Clients": {
        "asap-auth": {
          "Uri": ""
        },
        "asap-core": {
          "Uri": ""
        },
        "asap-github": {
          "Uri": ""
        },
        "asap-google": {
          "Uri": ""
        }
      }
    }
  }
}
